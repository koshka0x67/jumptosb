{
    "manifest_version": 2,
    "name": "Quick Search Jump",
    "version": "1.0",
    "description": "Jump to search bars on any website with customizable keyboard shortcuts",
    "permissions": ["activeTab", "storage", "<all_urls>"],
    "browser_action": {
        "default_popup": "popup.html",
        "default_title": "Quick Search Jump - Customize Key Binding"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"],
        "run_at": "document_end",
        "all_frames": true
      }
    ],
    "browser_specific_settings": {
      "gecko": {
        "id": "quick-search-jump@example.com"
      }
    }
}
  