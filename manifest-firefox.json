{
    "manifest_version": 2,
    "name": "Quick Search Jump",
    "version": "1.0",
    "description": "Jump to search bars on any website with the F key",
    "permissions": ["activeTab", "<all_urls>"],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"],
        "run_at": "document_end"
      }
    ],
    "browser_specific_settings": {
      "gecko": {
        "id": "quick-search-jump@example.com"
      }
    }
}
